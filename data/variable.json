  "MET_part_pz":{
    "Def": "if(MET_part_idx<0) return float(-99999.); else return sinh(MET_part_eta)*MET_part_pt;",
    "Label": ["Gen", "MC"],
    "Save":  []
  },

  "MET_reco_pz":{
    "Def": "MET_pz_reconstruction(bh_l1_pt, bh_l1_eta, bh_l1_phi, bh_met, bh_met_phi)",
    "Label": ["Reco","Normal"],
    "Save":[]
  },

  "MET_diff_pz":{
    "Def": "if(MET_part_idx<0) return float(-99999.); else return (MET_reco_pz - MET_part_pz);",
    "Label": ["Gen", "MC"],
    "Save":  []
  },

  "MET_reco_E":{
    "Def": "sqrt(bh_met*bh_met + MET_reco_pz*MET_reco_pz)",
    "Label": ["Reco", "Normal"],
    "Save": []
  },

  "bh_l1_E":{
    "Def": "sqrt(bh_l1_pt*bh_l1_pt*(cosh(bh_l1_eta)*cosh(bh_l1_eta)) + bh_l1_mass*bh_l1_mass)",
    "Label": ["Reco", "Normal"],
    "Save": []
  },

  "W_reco_E":{
    "Def": "MET_reco_E + bh_l1_E",
    "Label": ["Reco", "Normal"],
    "Save": []
  },

  "W_reco_px":{
    "Def": "bh_l1_pt*cos(bh_l1_phi) + bh_met*cos(bh_met_phi)",
    "Label": ["Reco", "Normal"],
    "Save":  []
  },

  "W_reco_py":{
    "Def": "bh_l1_pt*sin(bh_l1_phi) + bh_met*sin(bh_met_phi)",
    "Label": ["Reco", "Normal"],
    "Save":  []
  },

  "W_reco_pz":{
    "Def": "bh_l1_pt*sinh(bh_l1_eta) + MET_reco_pz",
    "Label": ["Reco", "Normal"],
    "Save":  []
  },

  "W_reco_mass":{
    "Def": "sqrt(W_reco_E*W_reco_E - W_reco_px*W_reco_px - W_reco_py*W_reco_py - W_reco_pz*W_reco_pz)",
    "Label": ["Reco", "Normal"],
    "Save":  []
  },

